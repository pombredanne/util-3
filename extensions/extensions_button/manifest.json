{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon_48x48.png",
    "default_title": "Extensions"
  },
  "description": "Provides quick icon access to your Chrome extensions.",
  "icons": {
    "128": "images/icon_128x128.png",
    "16": "images/icon_16x16.png",
    "48": "images/icon_48x48.png"
  },
  "manifest_version": 2,
  "name": "Extensions Button",
  "permissions": [
    "tabs",
    "*://*/"
  ],
  "version": "0.1"
}
