{
  "name": "Always Select",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Always Select prevents sites from disabling user selection via css.",
  "content_scripts":[
    {
      "all_frames": true,
      "css":[
        "css/style.css"
      ],
      "matches":[
        "*://*/*"
      ]
    }
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  }
}
